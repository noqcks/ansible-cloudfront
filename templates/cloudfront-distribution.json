{
  "Aliases": {
    "Quantity": 0,
    "Items": []
  },
  "DefaultRootObject": "",
  "PriceClass": "PriceClass_All",
  "Enabled": false,
  "Origins": {
    "Quantity": 1,
    "Items": [
      {
        "S3OriginConfig": {
          "OriginAccessIdentity": ""
          },
        {% if cloudfront_origin_type == 'ec2' %}
        "Id": "{{ cloudfront_origin_location }}",
        "DomainName": "cloudfront_origin_location"
        {% endif %}
        {% if cloudfront_origin_type == 's3' %}
        "Id": "S3-{{ cloudfront_origin_location}}",
        "DomainName": "{{ cloudfront_origin_location }}.s3.amazonaws.com"
        {% endif %}
      }
    ]
  },
  "DefaultCacheBehavior": {
    {% if cloudfront_origin_type == 'ec2' %}
    "TargetOriginId": "{{ cloudfront_origin_location }}",
    {% endif %}
    {% if cloudfront_origin_type == 's3' %}
    "TargetOriginId": "S3-{{ cloudfront_origin_location }}",
    {% endif %}
    "ForwardedValues": {
      "QueryString": false,
      "Cookies": {
        "Forward": "none",
        "WhitelistedNames": {
          "Quantity": 0,
          "Items": []
        }
      },
      "Headers": {
        "Quantity": 1,
        "Items": ["Origin"]
      }
    },
    "TrustedSigners": {
      "Enabled": false,
      "Quantity": 0,
      "Items": []
    },
    "ViewerProtocolPolicy": "allow-all",
    "MinTTL": 0,
    "AllowedMethods": {
      "Quantity": 2,
      "Items": ["GET","HEAD"],
      "CachedMethods": {
        "Quantity": 2,
        "Items": ["GET","HEAD"]
      }
    }
  },
  "ViewerCertificate": {
      "CloudFrontDefaultCertificate": true
  },
  "CustomErrorResponses": {
      "Quantity": 0
  },
  "Restrictions": {
      "GeoRestriction": {
          "RestrictionType": "none",
          "Quantity": 0
      }
  },
  "Comment": "Created with Ansible",
  "Enabled": true
}